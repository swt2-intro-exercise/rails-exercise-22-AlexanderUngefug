<h1> All Authors </h1>


<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Homepage</th>
    <th>Buttons</th>
  </tr>
  <% @authors.each do |author| %>
    <tr>
    <td> <%= author.id %></td>
    <td><%= link_to author.name, author %></td>
    <td><%= link_to author.homepage, author.homepage %></td>
    <td><%= link_to 'Edit', edit_author_path(author) %></td>
    </tr>
<% end %>
</table>
    <%= link_to 'New Author', new_author_path %>